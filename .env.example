# Docker Compose 环境变量配置（云服务版本）
# 复制此文件为 .env 并根据实际情况修改

# ==================== 应用版本信息 ====================
VERSION=v1.0.0
BUILD_TIME=
GIT_COMMIT=

# ==================== 应用服务配置 ====================
APP_PORT=8080
GIN_MODE=release

# ==================== 云数据库配置（主库）====================
# 用于存储订阅数据的主数据库
DB_HOST=your-cloud-mysql-host.com
DB_PORT=3306
DB_USER=your-db-user
DB_PASSWORD=your-db-password
DB_NAME=go_sub

# ==================== 云 Redis 配置 ====================
REDIS_HOST=your-cloud-redis-host.com
REDIS_PORT=6379
REDIS_PASSWORD=your-redis-password
REDIS_DB=0

# ==================== 数据源配置（远程 RDS）====================
# 用于执行订阅 SQL 的目标数据库
DATASOURCE_DEFAULT_HOST=testing-uhomes.rwlb.rds.aliyuncs.com
DATASOURCE_DEFAULT_PORT=3306
DATASOURCE_DEFAULT_USER=your-datasource-user
DATASOURCE_DEFAULT_PASS=your-datasource-password
DATASOURCE_DEFAULT_NAME=uhomes

DATASOURCE_UHOMES_HOST=testing-uhomes.rwlb.rds.aliyuncs.com
DATASOURCE_UHOMES_PORT=3306
DATASOURCE_UHOMES_USER=your-datasource-user
DATASOURCE_UHOMES_PASS=your-datasource-password
DATASOURCE_UHOMES_NAME=uhomes

# ==================== JWT 配置 ====================
JWT_SECRET=your-jwt-secret-key-change-this
JWT_EXPIRE_HOURS=24

# ==================== 其他配置 ====================
# Snowflake 节点 ID (0-1023)
SNOWFLAKE_NODE_ID=1

# 日志级别 (debug, info, warn, error)
LOG_LEVEL=info

# 时区
TZ=Asia/Shanghai
