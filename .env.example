# Docker Compose 环境变量配置
# 复制此文件为 .env 并根据实际情况修改

# 应用版本信息
VERSION=v1.0.0
BUILD_TIME=
GIT_COMMIT=

# MySQL 配置
MYSQL_ROOT_PASSWORD=password
MYSQL_USER=bisub
MYSQL_PASSWORD=bisub123
MYSQL_DATABASE=go_sub

# Redis 配置
REDIS_PASSWORD=

# 数据库配置（应用使用）
DB_HOST=mysql
DB_PORT=3306
DB_USER=root
DB_PASS=password
DB_NAME=go_sub

# Redis 配置（应用使用）
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASS=

# 服务配置
SERVER_PORT=8080
GIN_MODE=release

# JWT 密钥
JWT_SECRET=your-secret-key-change-in-production

# Snowflake 节点 ID (0-1023)
SNOWFLAKE_NODE_ID=1

# 日志级别 (debug, info, warn, error)
LOG_LEVEL=info

# 时区
TZ=Asia/Shanghai
